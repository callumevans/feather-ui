{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React starter project with hot reloading ready to go!",
  "main": "./src/app.js",
  "scripts": {
    "clean": "rimraf ./dist/",
    "start-dev": "webpack-dev-server -d --config ./webpack.config.js",
    "bundle-dev": "webpack --mode development --config ./webpack.config.js",
    "bundle-production": "webpack --mode production --config ./webpack.config.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.0-beta.2",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "normalize.css": "^8.0.0",
    "include-media": "^1.4.9",
    "react-hot-loader": "^4.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.20.3",
    "css-loader": "^0.28.11",
    "sass-loader": "^7.0.1",
    "url-loader": "^1.0.1",
    "file-loader": "^1.1.11"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Callum Evans",
  "license": "ISC"
}
